<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="preference_title" format="reference"/>
    <attr name="preference_summary" format="reference"/>
    <attr name="preference_icon" format="reference"/>
    <attr name="cropImageStyle" format="reference" />
    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showCircle" format="boolean" />
        <attr name="showHandles">
            <enum name="changing" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="PreviewOverlay">
        <attr name="preview_overlay_rect_width" format="dimension" />
        <attr name="preview_overlay_rect_height" format="dimension" />
    </declare-styleable>
</resources>
